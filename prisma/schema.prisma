model asset{
  id                Int     @id @default(autoincrement())
  type              String
  subtype           String
  manufacturer      String
  modelnum          String
  serialnum         String
  controlsys        String
  connection        String
  status            String 
  abrand            String
  createdAt         DateTime @default(now())
  updatedAt         DateTime @updatedAt
  part              part     @relation(fields: [partid], references: [idp])
  partid            String 
}

model part {
  id          Int      @id @default(autoincrement())
  idp         String   @unique
  brand       String
  quantity    String
  description String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
  asset       asset[]
}

